    "@types/js-yaml": "^3.12.1",
    "@types/keytar": "^4.4.0",
    "@types/nodegit": "^0.24.11",
    "@types/readline-sync": "^1.4.3",
    "@types/request": "^2.48.3",
    "chalk": "^2.4.2",
    "js-yaml": "^3.13.1",
